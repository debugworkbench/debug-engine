{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": true,
        "noImplicitAny": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "sourceMap": true,
        "outDir": "../lib"
    },
    "filesGlob": [
        "../typings/**/*.d.ts",
        "./**/*.ts"
    ],
    "files": [
        "engine.ts",
        "provider.ts"
    ]
}
